#!/bin/bash

# Check if the correct number of arguments is provided
if [ $# -ne 1 ]; then
    echo "Usage: $0 <file>"
    exit 1
fi

input_file="$1"

# Function to factorize a number and output its factorization
factorize() {
    local num="$1"
    for (( i=2; i<=num/2; i++ )); do
        if (( num % i == 0 )); then
            echo "$num=$i*$(($num / i))"
            return
        fi
    done
    echo "$num is prime"
}

# Read the input file line by line
while IFS= read -r line; do
    if [[ $line =~ ^[0-9]+$ ]]; then
        factorize "$line"
    else
        echo "Invalid input: $line"
    fi
done < "$input_file"
